{
 
  "name": "genius",
  "homepage": "https://maniaghili.github-io/Genius/",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "build:tailwind-main": "npx tailwindcss -i ./src/assets/styles/tailwind/input/input.css -o ./src/assets/styles/tailwind/output/output.css --watch",
    "build:tailwind-toggle": "npx tailwindcss -i ./src/assets/styles/tailwind/input/site/courses/toggle-box.css -o ./src/assets/styles/tailwind/output/site/courses/toggle-box.css",
    "build:tailwind-comment__answer": "npx tailwindcss -i ./src/assets/styles/tailwind/input/site/coursePreview/comment-answer.css -o ./src/assets/styles/tailwind/output/site/coursePreview/comment-answer.css",
    "t": "run-p  build:tailwind-main build:tailwind-toggle build:tailwind-comment__answer",
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint -c .eslintrc.json ./src/**/**/*.{ts,tsx} --fix",
    "preview": "vite preview"
  },
  "dependencies": {
    "aos": "^2.3.4",
    "axios": "^1.7.8",
    "izitoast": "^1.4.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.2",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.28.0",
    "sweetalert": "^2.1.2",
    "swiper": "^11.1.15"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/aos": "^3.0.7",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "gh-pages": "^6.2.0",
    "globals": "^15.11.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10"
  }
}
